<h1>{{title}}</h1>
<div class="container">
    <h1>{{message}}</h1>
    <table class="table table-striped">
        {{#each people}}
            <tr>
                <td>{{this.id}}</td>
                <td>
                    <a href='/persona/{{this.id}}'>
                        {{this.nombre}}
                    </a>
                </td>
            </tr>
            
        {{/each}}
    </table>
</div>
